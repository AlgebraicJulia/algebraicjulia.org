
@misc{brown_compositional_2022,
	title = {Compositional Exploration of Combinatorial Scientific Models},
	url = {http://arxiv.org/abs/2206.08755},
	doi = {10.48550/arXiv.2206.08755},
	abstract = {We implement a novel representation of model search spaces as diagrams over a category of models, where we have restricted attention to a broad class of models whose structure is presented by {\textbackslash}C-sets. (Co)limits in these diagram categories allow the creation of composite model spaces from more primitive spaces. We present a novel implementation of the computer algebra of finitely presented categories and diagram categories (including their limits and colimits), which formalizes a notion of model space exploration. This is coupled with strategies to facilitate the selection of desired models from these model spaces. We demonstrate our framework by generating a tool which fits experimental data, searching an epidemiology-relevant subspace of mass-action kinetic models.},
	number = {{arXiv}:2206.08755},
	publisher = {{arXiv}},
	author = {Brown, Kristopher and Hanks, Tyler and Fairbanks, James},
	urldate = {2023-01-04},
	date = {2022-06-07},
	eprinttype = {arxiv},
