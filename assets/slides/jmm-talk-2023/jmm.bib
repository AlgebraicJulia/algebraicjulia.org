	eprinttype = {arxiv},
	eprint = {2204.01843 [math-ph]},
	keywords = {Mathematical Physics, Mathematics - Category Theory},
	file = {arXiv Fulltext PDF:/home/o/Zotero/storage/YQYRYHHK/Patterson et al. - 2022 - A diagrammatic view of differential equations in p.pdf:application/pdf;arXiv.org Snapshot:/home/o/Zotero/storage/K72KIM48/2204.html:text/html},
